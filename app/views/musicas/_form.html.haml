= form_for @musica do |f|
  - if @musica.errors.any?
    #error_explanation
      %h2= "#{pluralize(@musica.errors.count, "error")} prohibited this musica from being saved:"
      %ul
        - @musica.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :embed_code
    = f.text_area :embed_code, :cols => 50, :rows => 10 
  .actions
    = f.submit 'Save'
