- content_for :css do
  = stylesheet_link_tag 'megafolio/fancybox/jquery.fancybox.css'

- content_for :javascript do
  = javascript_include_tag 'megafolio/fancybox/jquery.fancybox.pack.js'
  :javascript
    $(document).ready(function() {
      // THE FANCYBOX PLUGIN INITALISATION
      jQuery(".fancybox").fancybox();
    });

